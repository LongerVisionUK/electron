{
  "name": "parent",
  "version": "1.0.0",
  "description": "Electron workspace",
  "main": "index.js",
  "repository": "https://github.com/electron-lang/electron",
  "author": "David Craven <david@craven.ch>",
  "license": "ISC",
  "private": true,
  "workspaces": [
    "electron",
    "electron/src/tests/pass",
    "celllib",
    "language-server",
    "schematic-diagram",
    "theia-electron",
    "browser-app",
    "desktop-app",
    "website",
    "examples/electron",
    "examples/tinyFPGA",
    "examples/blinky",
    "examples/picorv32",
    "examples/picosoc",
    "examples/dac",
    "examples/dacImpl"
  ],
  "resolutions": {
    "sprotty": "0.4.0"
  },
  "scripts": {
    "prepare": "lerna run prepare && yarn run docs",
    "docs": "node magic-readme.js",
    "test": "lerna run test",
    "cov": "lerna run cov",
    "publish": "yarn && yarn test && yarn publish:latest",
    "publish:latest": "lerna publish patch --registry=https://registry.npmjs.org/ --npm-tag=latest --yes --no-git-tag-version --force-publish=*",
    "publish:next": "lerna publish --registry=https://registry.npmjs.org/ --canary --npm-tag=next --yes --no-git-tag-version --force-publish=*"
  },
  "devDependencies": {
    "lerna": "^3.0.1",
    "markdown-magic": "^0.1.25",
    "markdown-magic-subpackage-list": "^1.1.1"
  }
}
